{
    "name": "@cicis/react-components",
    "version": "1.0.72",
    "description": "A React component library for cic Identity Server frontend apps implementing Semantic UI Framework",
    "keywords": [
        "cic",
        "cic IS",
        "cic Identity Server",
        "cic Identity Apps",
        "react",
        "react-components",
        "component-library"
    ],
    "main": "dist/src/index.js",
    "types": "dist/typings/index.d.ts",
    "files": [
