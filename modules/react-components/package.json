    ],
    "scripts": {
        "start:storybook": "NODE_PATH=src start-storybook -p 6007 -c .storybook -s ./dist",
        "build:storybook": "build-storybook",
        "build": "npm run clean && npm run compile",
        "clean": "rimraf ./dist",
        "compile": "run-script-os",
        "compile:win32": "..\\..\\node_modules\\.bin\\tsc -p tsconfig.json",
        "compile:default": "../../node_modules/.bin/tsc -p tsconfig.json",
        "start": "npm run start:storybook",
        "lint": "tslint -t stylish --project ."
    },
    "author": "cic",
    "license": "Apache-2.0",
